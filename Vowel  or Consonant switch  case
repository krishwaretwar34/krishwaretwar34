 
#include <stdio.h>

int main()
{
    char   vowel;
    printf("Enter your ALphabet:");
    scanf("%c",&vowel);
     switch(vowel){
         case 'a':
         printf("Vowel");
         break;
         case 'e':
         printf("Vowel");
         break;
         case 'i':
         printf("vowel");
         break;
         case 'o':
         printf("Vowel");
         break;
         case 'u':
         printf("Vowel");
         break;
         case 'A':
         printf("Vowel");
         break;
         case 'E':
         printf("Vowel");
         break;
         case 'I':
         printf("Vowel");
         break;
         case 'O':
         printf("Vowel");
         break;
         case 'U':
         printf("Vowel");
         break;
         
         default:
         printf("not a vowel");
     }
    
    

    return 0;
}
